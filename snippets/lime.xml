  <remote name="gitlab"
          fetch="https://gitlab.com" />

  <!-- Magma -->
  <project path="manifest" name="manifest" remote="lime" />
  <project path="build/make" name="build_make" remote="lime" >
    <copyfile src="core/root.mk" dest="Makefile" />
