<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote name="lime"
          fetch="https://github.com/AospLime"
          revision="ten" />

  <remote name="github"
          fetch="https://github.com/AospLime"
          revision="ten" />
	
  <remote name="lime-devices"
          fetch="https://github.com/AospLime-Devices"
